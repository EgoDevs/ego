{
    "name": "ego",
    "version": "0.0.1",
    "description": "ego",
    "private": true,
    "scripts": {
        "ego:init": "ego --init",
        "ego:clean": "ego --clean",
        "ego:build": "ego --build",
        "ego:build:debug": "ego --build --featurs=debug",
        "switch:mac": "cp configs/mac.json configs/local.json",
        "switch:aws": "cp configs/aws.json configs/local.json",

        "ego:create": "ego --create",
        "ego:install": "ego --install",
        "ego:reinstall": "ego --reinstall",
        "ego:upgrade": "ego --upgrade",
        "migrate": "jest --testPathPattern=migrations --verbose -t",
        "ego:script": "jest --testPathPattern=scripts --verbose -t",
        "ego:deploy": "jest --testPathPattern=deploys --verbose -t",
        "ego:run": "pnpm run ego:create && pnpm run ego:install && pnpm run ego:deploy",

        "ego:create:aws": "ego --create --env=custom",
        "ego:install:aws": "ego --install --env=custom",
        "ego:reinstall:aws": "ego --reinstall --env=custom",
        "ego:upgrade:aws": "ego --upgrade --env=custom",
        "migrate:aws": "NODE_ENV=custom jest --testPathPattern=migrations --verbose -t",
        "ego:deploy:aws": "NODE_ENV=custom jest --testPathPattern=deploys --verbose -t",
        "ego:script:aws": "NODE_ENV=custom jest --testPathPattern=scripts --verbose -t",

        "ego:create:staging": "ego --create --env=testnet",
        "ego:install:staging": "ego --install --env=testnet",
        "ego:reinstall:staging": "ego --reinstall --env=testnet",
        "ego:upgrade:staging": "ego --upgrade --env=testnet",
        "migrate:staging": "NODE_ENV=testnet jest --testPathPattern=migrations --verbose -t",
        "ego:deploy:staging": "NODE_ENV=testnet jest --testPathPattern=deploys --verbose -t",
        "ego:script:staging": "NODE_ENV=testnet jest --testPathPattern=scripts --verbose -t",

        "ego:create:main": "ego --create --env=mainnet",
        "ego:install:main": "ego --install --env=mainnet",
        "ego:reinstall:main": "ego --reinstall --env=mainnet",
        "ego:upgrade:main": "ego --upgrade --env=mainnet",
        "migrate:main": "NODE_ENV=mainnet jest --testPathPattern=migrations --verbose -t",
        "ego:deploy:main": "NODE_ENV=mainnet jest --testPathPattern=deploys --verbose -t",
        "ego:script:main": "NODE_ENV=mainnet jest --testPathPattern=scripts --verbose -t"
    },
    "author": "egoDevs@github.com",
    "license": "ISC",
    "dependencies": {
        "@dfinity/agent": "^0.15.5",
        "@dfinity/candid": "^0.15.5",
        "@dfinity/identity": "^0.15.5",
        "@dfinity/principal": "^0.15.5",
        "@ego-js/cli": "^2.2.45",
        "@ego-js/utils": "^2.2.45",
        "@ego-js/core": "^2.2.45",
        "@swc/cli": "^0.1.59",
        "@swc/core": "^1.3.29",
        "@swc/jest": "^0.2.24",
        "@types/mockjs": "^1.0.7",
        "@types/yargs": "^17.0.17",
        "bip32": "^3.1.0",
        "bip39": "^3.0.4",
        "buffer": "^6.0.3",
        "crc": "^4.1.1",
        "cross-env": "^7.0.3",
        "cross-fetch": "^3.1.5",
        "js-sha256": "^0.9.0",
        "mockjs": "^1.1.0",
        "starkbank-ecdsa": "^1.1.5",
        "tiny-secp256k1": "^2.2.1",
        "yargs": "^17.6.2"
    },
    "devDependencies": {
        "@types/jest": "^28.0.3",
        "@types/node": "^17.0.45",
        "@types/shelljs": "^0.8.11",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "eslint": "8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^28.0.3",
        "prettier": "^2.8.1",
        "shelljs": "^0.8.5",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.2",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.9.4"
    }
}