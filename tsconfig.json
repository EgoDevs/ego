{
    "compilerOptions": {
      "allowJs": true,
      "checkJs": false,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "lib": ["ES2019", "DOM"],
      "strict": true,
      "module": "commonjs", //ii
      "target": "es2018", //ii
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": false,
      "outDir": "/",
      "rootDir": ".",
      "importHelpers": true,
      "jsx": "react-jsx",
      "sourceMap": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["clients/*"]
      },
      "noImplicitAny": false,
      "downlevelIteration": true
    },
    "include": [
      "clients/**/*", 
    ],
    "exclude": []
  }
  